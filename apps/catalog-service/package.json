{
  "name": "@shoplite/catalog-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e .env -- ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "postbuild": "node -e \"require('fs').cpSync('src/generated','dist/generated',{recursive:true})\"",
    "start:local": "dotenv -e .env -- node dist/index.js",
    "start:prod": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "dotenv -e .env -- prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:deploy:test": "dotenv -e .env.test -- prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.16.0",
  "dependencies": {
    "@prisma/client": "6.19.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.2",
    "dotenv-cli": "^11.0.0",
    "prisma": "6.19.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
