generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(uuid())
  title     String
  price     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
